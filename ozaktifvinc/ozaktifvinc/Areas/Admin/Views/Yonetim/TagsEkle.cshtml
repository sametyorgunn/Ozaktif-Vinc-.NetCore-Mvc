@model ozaktifvinc.Models.MetaTag
@{
    ViewData["Title"] = "TagsEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<form method="post" enctype="multipart/form-data">

    <div hidden class="form-group">
        <label for="exampleInputPassword1">Id</label>
        <input type="text" class="form-control col-sm-3" name="Id" value="@Model.Id" id="exampleInputPassword1">
    </div>
     <div class="form-group">
        <label for="exampleInputPassword1">Title</label>
        <input type="text" class="form-control col-sm-3" name="Title" value="@Model.Title">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Content</label>
        <input type="text" class="form-control col-sm-3" name="Content" value="@Model.Content">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Keywords</label>
        <input type="text" class="form-control col-sm-3" value="@Model.Keyword" name="Keyword">
    </div>
    <br />
    <button class="btn btn-primary" type="submit">Kaydet</button>
</form>
