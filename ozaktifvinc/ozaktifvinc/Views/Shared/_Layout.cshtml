    <!-- Vendor CSS Files -->
    <link href="~/UpConstruction/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/UpConstruction/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/UpConstruction/assets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet">
    <link href="~/UpConstruction/assets/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/UpConstruction/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/UpConstruction/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/UpConstruction/assets/css/main.css" rel="stylesheet">

    <!-- =======================================================
