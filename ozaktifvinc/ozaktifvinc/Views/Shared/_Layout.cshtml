<body>

    <!-- ======= Header ======= -->
    <header id="header" class="header d-flex align-items-center">
        <div class="container-fluid container-xl d-flex align-items-center justify-content-between">

            <a href="/Home/Index" class="logo d-flex align-items-center">
                <!-- Uncomment the line below if you also wish to use an image logo -->
                <!-- <img src="assets/img/logo.png" alt=""> -->
                <h1>ÖZAKTİF VİNÇ<span></span></h1>
            </a>

            <i class="mobile-nav-toggle mobile-nav-show bi bi-list"></i>
            <i class="mobile-nav-toggle mobile-nav-hide d-none bi bi-x"></i>
            <nav id="navbar" class="navbar">
                <ul>
                    <li><a href="#hero" class="active">Anasayfa</a></li>
                    <li><a href="#alt-services">Hakkımızda</a></li>
                    <li><a href="#services">Hizmetler</a></li>
                    <li><a href="#projects">Galeri</a></li>
                    <li><a href="#get-started">İletişim</a></li>
                </ul>
            </nav><!-- .navbar -->

        </div>
    </header><!-- End Header -->
    @RenderBody()
    <!-- ======= Footer ======= -->
    @await Component.InvokeAsync("Footer")
    <!-- End Footer -->

    <a href="#" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <div id="preloader"></div>

    <!-- Vendor JS Files -->
    <script src="~/UpConstruction/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/UpConstruction/assets/vendor/aos/aos.js"></script>
    <script src="~/UpConstruction/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/UpConstruction/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/UpConstruction/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/UpConstruction/assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/UpConstruction/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/UpConstruction/assets/js/main.js"></script>

